struct IntParam {
	int val;
};
const INT_VALUE = 0x3169803f;

struct CondVarPtr {
	unsigned hyper cond;
	unsigned hyper mutex;
};

serverprocs {
	ping() = 1;
	loop_int(IntParam, IntParam) = 2;
	check_int(IntParam) = 3;
	error(void, IntParam) = 4;
	invalidate_ops() = 5;
	trigger_callback() = 6;
}

servermsgs {
	notify(CondVarPtr) = 1;
}

clientprocs {
	client_check_int(IntParam) = 1;
}

clientmsgs {
	client_notify(CondVarPtr) = 1;
}
