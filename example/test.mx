struct TestRequest {
	int num;
};

struct TestReply {
	int num;
};

struct TestNotify {
	int num;
};

serverprocs {
	query(TestRequest, TestReply) = 1;
	query_async_reply(TestRequest, TestReply) = 2;
	call(TestRequest) = 3;
	error(void, TestReply) = 4;
	invalidate_ops() = 5;
	ping() = 6;
}

servermsgs {
	notify(TestNotify) = 1;
}
